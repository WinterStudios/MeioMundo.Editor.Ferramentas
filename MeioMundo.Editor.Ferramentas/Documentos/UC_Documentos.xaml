<UserControl x:Class="MeioMundo.Editor.Ferramentas.Documentos.UC_Documentos"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MeioMundo.Editor.Ferramentas.Documentos"
             mc:Ignorable="d" 
             HorizontalAlignment="Stretch">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="32"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Menu Grid.Row="0" OverridesDefaultStyle="False" >
            <MenuItem Header="Teste" Click="MenuItem_Click" />
            <MenuItem Header="Editor" Click="MenuItem_Click_1"/>
        </Menu>
        <local:Modelo_Manuais x:Name="modelo" Grid.Row="1" />
    </Grid>
</UserControl>
